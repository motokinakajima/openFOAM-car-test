FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    frontAndBack
    {
        type            slip;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    lowerWall
    {
        type            zeroGradient;
    }
    upperWall
    {
        type            slip;
    }
    mainBody
    {
        type            zeroGradient;
    }
    FL
    {
        type            zeroGradient;
    }
    FR
    {
        type            zeroGradient;
    }
    RL
    {
        type            zeroGradient;
    }
    RR
    {
        type            zeroGradient;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 0;
    }
}


