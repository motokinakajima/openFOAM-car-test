FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (20 0 0);

boundaryField
{
    frontAndBack
    {
        type            slip;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform List<vector> 0;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform List<vector> 0;
        value           nonuniform List<vector> 0;
    }
    lowerWall
    {
        type            fixedValue;
        value           uniform (20 0 0);
    }
    upperWall
    {
        type            slip;
    }
    mainBody
    {
        type            noSlip;
    }
    FL
    {
        type            noSlip;
    }
    FR
    {
        type            noSlip;
    }
    RL
    {
        type            noSlip;
    }
    RR
    {
        type            noSlip;
    }
    procBoundary4to0
    {
        type            processor;
        value           uniform (20 0 0);
    }
    procBoundary4to1
    {
        type            processor;
        value           uniform (20 0 0);
    }
    procBoundary4to3
    {
        type            processor;
        value           uniform (20 0 0);
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform (20 0 0);
    }
}


